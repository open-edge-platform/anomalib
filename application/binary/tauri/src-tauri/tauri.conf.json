{
  "$schema": "../../../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "anomalib-studio",
  "version": "0.1.0",
  "identifier": "com.anomalib.studio",
  "build": {
    "frontendDist": "ui",
    "devUrl": "localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Anomalib Studio",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.ico", "icons/icon.png"]
  }
}
